{
  "hosting": {
    "public": "dist/kanzai/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "kanzai-nest-ssr",
          "region": "europe-west2"
        }
      }
    ]
  }
}
